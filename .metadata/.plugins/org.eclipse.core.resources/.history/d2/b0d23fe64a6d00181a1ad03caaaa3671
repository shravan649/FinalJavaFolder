public class WebDriverDemo {

	public static void main(String[] args) {
	System.setProperty("webdriver.chrome.driver", "D:\\javaflp crosskiling(9-4-18)");
 WebDriver driver=new ChromeDriver();
 driver.get("http://www.google.com");
	}

}
